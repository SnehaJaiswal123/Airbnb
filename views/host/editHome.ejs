<%- include('../partials/head') %>
  </head>
  <body>
    <h1>Edit your Home:</h1>
     <form action="/host/edit-home" method="post">
      <input type="text" name="houseName" placeholder="House Name" value=<%=home.houseName %> />
      <input type="number" name="price" placeholder="Price Per Night in Rs." value=<%=home.price %> />
      <input type="text" name="location" placeholder="Location" value=<%=home.location %> >
      <input type="number" step="0.01" name="rating" placeholder="Ratings" value=<%=home.rating %> />
      <input type="text" name="description" placeholder="Description" value=<%=home.description %> />
      <input type="text" name="photo_url" placeholder="Image URL" value=<%=home.photo_url %> />
      <input type="hidden" name="_id" value=<%=home._id %>>
      <button type="submit">Submit</button>
    </form>
  </body>
</html>