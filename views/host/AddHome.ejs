<%- include('../partials/head') %>
  </head>
  <body>
    <h1>Add your home:</h1>
     <form action="/host/home-added" method="post">
      <input type="text" name="houseName" placeholder="House Name">
      <input type="number" name="price" placeholder="Price Per Night in Rs.">
      <input type="text" name="location" placeholder="Location">
      <input type="number" step="0.01"  name="rating" placeholder="Ratings"/>
      <input type="text" name="description" placeholder="Description">
      <input type="text" name="photo_url" placeholder="Image URL">
      <button type="submit">Submit</button>
    </form>
  </body>
</html>